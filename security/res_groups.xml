<odoo>

    <record model="ir.module.category" id="module_category_gitlab_connector">
        <field name="name">GitLab Connector</field>
        <field name="sequence">20</field>
    </record>

    <record id="base.group_no_one" model="res.groups">
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>
    <record id="group_gitlab_connector_user" model="res.groups">
        <field name="name">Connector Gitlab User</field>
        <field name="category_id" ref="module_category_gitlab_connector"/>
    </record>
    <record id="group_gitlab_connector_manager" model="res.groups">
        <field name="name">Connector Gitlab Manager</field>
        <field name="category_id" ref="module_category_gitlab_connector"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
        <field name="implied_ids" eval="[(4, ref('gitlab_connector.group_gitlab_connector_user'))]"/>
    </record>

</odoo>
